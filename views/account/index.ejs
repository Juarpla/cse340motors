<% if (title) { %>
<h1><%= title %></h1>
<% } else { res.redirect('/') } %> <%- messages() %>
<br />
<% if (errors) { %>
<ul class="notice">
  <% errors.array().forEach(error => { %>
  <li><%= error.msg %></li>
  <% }) %>
</ul>
<br />
<% } %>
<div>
  <h1>Welcome Here</h1>
  <p>You're logged in</p>
  <br />
</div>
